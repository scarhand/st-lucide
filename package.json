    "lucide": "^0.91.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "26.0.15",
    "@types/puppeteer": "5.4.0",
    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "puppeteer": "5.4.1"
  }
